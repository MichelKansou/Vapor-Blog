#extend("Layouts/admin")

#export("head") {
	<title>My App</title>
}

#export("body") {
    <div class="list-container">
        #if(post) {
            <article class="post">
                <h2 class="title">#(post.title)</h2>
                <p class="content">#(post.content)</p>
            </article>
        }
    </div>
}
